#include <stdio.h>

int main() {
    int arr[] = {3, -5, 2, -8, 1, -4};
    int n = sizeof(arr) / sizeof(arr[0]);

    printf("Non-negative elements: ");
    for (int i = 0; i < n; i++) {
        if (arr[i] >= 0) {
            printf("%d ", arr[i]);
        }
    }

    printf("\nNegative elements: ");
    for (int i = 0; i < n; i++) {
        if (arr[i] < 0) {
            printf("%d ", arr[i]);
        }
    }

    return 0;
}
